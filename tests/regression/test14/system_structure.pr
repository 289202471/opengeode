/* CIF Keep Specific Geode ASNFilename 'dataview-uniq.asn' */
USE Datamodel;
SYSTEM sequences;

    SIGNAL run;

    CHANNEL c
        FROM ENV TO sequences WITH run;
    ENDCHANNEL;

    BLOCK basic;

        SIGNALROUTE r
            FROM ENV TO sequences WITH run;
        CONNECT c and r;

        PROCESS sequences REFERENCED;

    ENDBLOCK;

ENDSYSTEM;
