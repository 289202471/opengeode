                /* CIF decision (222, 253), (106, 50) */
                decision present (val);
                    /* CIF ANSWER (241, 323), (70, 24) */
                    else:
                enddecision;
                /* CIF NEXTSTATE (243, 362), (64, 35) */
                NEXTSTATE hop;
