            dcl val MyChoice,  det MyEnum;
            
            dcl choice_selectors MyChoice_selection := foo;
            /* CIF ENDTEXT */
            /* CIF START (240, 89), (70, 35) */
            START;
                /* CIF task (160, 144), (229, 35) */
                task choice_selectors := present(val);
                /* CIF decision (222, 194), (106, 50) */
                decision present (val);
                    /* CIF ANSWER (195, 264), (70, 24) */
                    (foo):
                    /* CIF ANSWER (285, 264), (70, 24) */
                    (bar):
                enddecision;
                /* CIF decision (222, 303), (106, 50) */
                decision present (val);
                    /* CIF ANSWER (166, 373), (129, 24) */
                    (choice_selectors):
                    /* CIF ANSWER (315, 373), (70, 24) */
                    else:
                enddecision;
                /* CIF decision (210, 412), (129, 50) */
                decision choice_selectors;
                    /* CIF ANSWER (223, 482), (103, 24) */
                    (present(val)):
                    /* CIF ANSWER (346, 482), (70, 24) */
                    else:
                enddecision;
                /* CIF task (233, 521), (83, 35) */
                task det := foo;
                /* CIF task (189, 576), (171, 40) */
                task choice_selectors := bar;
                /* CIF task (113, 636), (324, 40) */
                task choice_selectors := to_selector (det, MyChoice);
                /* CIF task (124, 696), (302, 40) */
                task det := to_enum (choice_selectors, MyEnum);
                /* CIF NEXTSTATE (243, 751), (64, 35) */
                NEXTSTATE hop;
            /* CIF state (528, 360), (70, 35) */
            state hop;
            endstate;
