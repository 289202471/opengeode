                    /* CIF PROCEDURECALL (281, 336), (100, 35) */
                    CALL hehe(hihi);
                    /* CIF OUTPUT (286, 386), (89, 35) */
                    OUTPUT we(test);
                    /* CIF NEXTSTATE (296, 436), (70, 35) */
                    NEXTSTATE wait;
            ENDSTATE;
