                /* CIF ENDTEXT */
                /* CIF START (164, 113), (70, 35) */
                START;
                    /* CIF OUTPUT (154, 163), (89, 35) */
                    OUTPUT we(test);
                    /* CIF OUTPUT (155, 213), (88, 35) */
                    OUTPUT we(tmp)
                    /* CIF COMMENT (263, 213), (269, 35) */
