                    /* CIF RETURN (181, 263), (35, 35) */
                    RETURN ;
            ENDPROCEDURE;
            /* CIF START (155, 216), (70, 35) */
            START;
                /* CIF NEXTSTATE (155, 266), (70, 35) */
                NEXTSTATE wait;
            /* CIF STATE (296, 231), (70, 35) */
            STATE wait;
                /* CIF INPUT (289, 286), (84, 35) */
