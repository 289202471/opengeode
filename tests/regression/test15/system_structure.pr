/* CIF Keep Specific Geode ASNFilename 'dataview-uniq.asn' */
USE Datamodel;
SYSTEM controlflow;

    SIGNAL run;

    CHANNEL c
        FROM ENV TO controlflow WITH run;
    ENDCHANNEL;

    BLOCK basic;

        SIGNALROUTE r
            FROM ENV TO controlflow WITH run;
        CONNECT c and r;

        PROCESS controlflow REFERENCED;

    ENDBLOCK;

ENDSYSTEM;
